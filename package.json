      "out/**/*.js"
    ],
    "targets": [
      "node16-win-x64"
    ]
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@inquirer/prompts": "^7.2.3",
    "commander": "^13.0.0"
  },
  "devDependencies": {
    "@types/node": "^22.10.7",
    "pkg": "^5.8.1",
    "tsx": "^4.19.2",
    "typescript": "^5.7.3",
    "vite": "^6.0.9"
  }
}